    "publint": "^0.2.3",
    "sass": "^1.68.0",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
