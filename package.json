{
  "name": "@radar-azdelta/svelte-athena-search",
  "version": "0.0.18",
  "repository": {
    "type": "git",
    "url": "https://github.com/RADar-AZDelta/svelte-Athena-search.git"
  },
  "homepage": "https://radar-azdelta.github.io/svelte-Athena-search/",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev --host=127.0.0.1 --port=5178",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "package:watch": "svelte-package --watch",
    "prepublishOnly": "npm run package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.2.2",
    "@types/cookie": "^0.5.1",
    "@types/lodash.debounce": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.2.3",
    "sass": "^1.68.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@radar-azdelta/svelte-datatable": "^0.0.48",
    "@sveltejs/kit": "^1.20.4",
    "lodash.debounce": "^4.0.8",
    "svelte": "^4.0.5"
  },
  "files": ["dist", "dist/index.js", "dist/index.d.ts"],
  "svelte": "index.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
    "./Search.svelte": {
      "types": "./dist/components/Search.svelte.d.ts",
      "svelte": "./dist/components/Search.svelte"
    }
	}
}
