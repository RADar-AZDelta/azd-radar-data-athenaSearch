    "dist/index.js",
    "dist/index.d.ts"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
